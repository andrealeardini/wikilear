{
  "name": "wikilear",
  "version": "0.0.1",
  "description": "The Wikilear web site",
  "main": "main.js",
  "scripts": {
    "start": "npm run eleventy-watch",
    "serve": "web-dev-server --root-dir dist --node-resolve --open",
    "build": "npm run eleventy",
    "clean": "set -e && find ./dist -mindepth 1 ! -regex '^./dist/images\\(/.*\\)?' -delete || echo dist not found",
    "eleventy": "NODE_ICU_DATA='./node_modules/full-icu' NODE_ENV=production npx @11ty/eleventy",
    "eleventy-watch": "NODE_ICU_DATA='./node_modules/full-icu' npx @11ty/eleventy --serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Andrea Leardini",
  "license": "MIT",
  "dependencies": {
    "netlify-plugin-cache": "^1.0.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.2",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@types/luxon": "^2.0.5",
    "@web/dev-server": "^0.1.25",
    "autoprefixer": "^10.3.1",
    "clean-css": "^4.2.3",
    "csp-hash-generator": "^0.0.3",
    "csv-parse": "^4.16.0",
    "datauri": "^3.0.0",
    "eslint": "^7.32.0",
    "full-icu": "^1.3.4",
    "hasha": "^5.2.2",
    "html-minifier": "^4.0.0",
    "image-size": "^0.9.7",
    "jsdom": "^16.7.0",
    "luxon": "^1.28.0",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-attrs": "^3.0.3",
    "normalize.css": "^8.0.1",
    "postcss": "^8.3.6",
    "sharp": "^0.27.2",
    "tailwindcss": "^2.2.7",
    "terser": "^5.9.0",
    "watch": "^0.13.0"
  }
}
